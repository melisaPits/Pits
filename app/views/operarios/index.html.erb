<div class="row">
      <div class="col s12">
            <blockquote><h3>Catálogo Usuarios</h3></blockquote>
      </div>
</div>
<div class="row">
	<div class="col s12">
		<ul class="tabs">
			<li class="tab col s6"><a class="active" href="#test1">Registrar</a></li>
			<li class="tab col s6"><a href="#test2">Registrados</a></li>
		</ul>
	</div>

	<div id="test1" class="col s12">
        	<%= render :partial => "new"%>
	</div>

	<div id="test2" class="col s12">
			<h5 style="background: #bbdefb;">Listado de usuarios</h5>
			<table id="tablausuarios">
				<thead>
					<tr>
						<th>Nombre</th>
						<th>Puesto</th>
						<th>Teléfono</th>
						<th>Correo</th>
					</tr>
				</thead>
				<tbody>
					<%@usuarios.each do |s| %>
					<tr>
						<td><%= s.nombre %></td>
						<td><%= s.tipo %></td>
						<td><%= s.celular %></td>
						<td><%= s.email %></td>
					</tr>
					<% end %>
				</tbody>
			</table>
	</div>
</div>